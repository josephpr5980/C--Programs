using System;
using System.Collections.Generic;

public class Program
{
    static void Main(string[] args)
    {
        int[] numbers = {1, 4, 6, 8, 9, 11, 21, 19, 12, 13, 14, 15, 16, 20 ,0};
        int maxlength  = 0 ;
        int startofMax = 0;
        
        
        HashSet<int> numset = new HashSet<int>(numbers);
     
        foreach(int num in numbers)
        {
            if(!numset.Contains(num-1))
            {
                int currentnum = num;
                int currentlength  = 1;
                
                while(numset.Contains(currentnum + 1))
                {
                    currentnum++;
                    currentlength++;
                }
                
                if(currentlength > maxlength)
                {
                    maxlength = currentlength;
                    startofMax = num;
                }
            }
        }
        Console.WriteLine("maxlength is :" + maxlength);
        int[] longestarray  = new int[maxlength];
        for(int i = 0; i<maxlength; i++)
        {
           longestarray[i] = startofMax+i;
        }
        foreach(int val in longestarray)
        {
            Console.Write(val+", ");
            Console.WriteLine();
        }
        
    }
    
}


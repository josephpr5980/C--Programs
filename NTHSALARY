SELECT EMP_ID, EMP_NAME, SALARY 
FROM EMPLOYEES E1
ORDER BY SALARY DESC
LIMIT 1 OFFSET N-1

WITH CTE AS (
  SELECT SALARY, DENSE_RANK() OVER (ORDER BY SALARY DESC) AS RANK 
	FROM EMPLOYEES
)
SELECT SALARY AS NTHSALARY
FROM CTE 
WHERE RANK =3

WITH CTE AS (
  SELECT SALARY, rOW_nUMBER() OVER (ORDER BY SALARY DESC) AS ROWNUMBER 
	FROM (SELECT DISTINCT SALARY FROM EMPLOYEES) AS DISTINCTSALARIES
)
SELECT SALARY AS NTHSALARY
FROM CTE 
WHERE ROWNUMBER =3

